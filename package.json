{
  "name": "deckdev",
  "description": "Stream Deck Development Tool",
  "version": "0.3.2",
  "author": "Houssam Haidar @hhaidar",
  "bin": {
    "deckdev": "./bin/run"
  },
  "homepage": "https://github.com/hhaidar/deckdev",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "hhaidar/deckdev",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@types/debounce": "^1.2.1",
    "awesome-debounce-promise": "^2.1.0",
    "chokidar": "^3.5.3",
    "chrome-remote-interface": "^0.32.1",
    "debounce": "^1.2.1",
    "execa": "^5.1.1",
    "fs-extra": "^11.1.1",
    "poll-until-promise": "^4.1.0",
    "rage-edit": "^1.2.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.21",
    "@types/chai": "^4",
    "@types/chrome-remote-interface": "^0.31.9",
    "@types/fs-extra": "^11.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.20",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^9",
    "oclif": "^3",
    "prettier": "^2.8.7",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2"
  },
  "oclif": {
    "bin": "deckdev",
    "dirname": "deckdev",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "scripts": {
    "dev": "./bin/dev",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/hhaidar/deckdev/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
